{
  "expo": {
    "name": "Shared Expenses",
    "description": "Now you can share expenses with your friends and forget about who owes whom because Shared Expenses helps you with it",
    "slug": "Shared Expenses",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.sharedexpenses"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
